<?xml version="1.0"?>
<extension key="emailnotification" type="module">
  <file>emailnotification</file>
  <name>Email Notification</name>
  <description>Send email notifications for activities</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Patrick Figel</author>
    <email>pfigel@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/emailnotification</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/emailnotification</url>
    <url desc="Support">https://github.com/greenpeace-cee/emailnotification/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-10-10</releaseDate>
  <version>1.0.0-alpha.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Emailnotification</namespace>
    <format>23.02.1</format>
    <angularModule>crmEmailnotification</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
